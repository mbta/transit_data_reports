next_stops =
  settings.stop_ids
  |> Enum.flat_map(&Map.fetch!(first_to_next_stop, &1))
  |> MapSet.new()

vehicle_positions =
  :VehiclePositions
  |> GlidesReport.Util.stream_values()
  |> Stream.filter(&GlidesReport.VehiclePosition.relevant?/1)
